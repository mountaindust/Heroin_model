@article{Ma,
	Abstract = {In this paper, the dynamical properties of a heroin model with nonlinear contact rate are discussed. We analyze the types of the equilibria and show that the model exhibits numerous kinds of bifurcation, such as the saddle-node bifurcation, the Hopf bifurcation, Bogdanov--Takens bifurcation of codimension 2 and so on as the parameters values vary. These results have certain effect to control the heroin prevalence.},
	Author = {Mingju Ma and Sanyang Liu and Jun Li},
